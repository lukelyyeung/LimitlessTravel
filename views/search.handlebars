<template id="template1">

    <div class="card text-center">
        <form>
            <div class="main card-header">
                <h3 class="card-title">
                    <i class="material-icons">filter_list</i>
                    Search
                </h3>
            </div>

            <div class="card-block">

                <div class="slidecontainer">
                    <label for="example-datetime-local-input" class="col-12 col-form-label d-flex align-items-center justify-content-center">
                        <i class="material-icons">attach_money</i>
                        Budget
                    </label>

                    <input type="range" value="5000" step="100" min="1000" max="50000" style='width: 300px'>
                    <br>
                    <span>$ </span>
                    <output style="font-size: 32px">5000</output>
                </div>

                <div class="row">
                    <div class="col-6">
                        <div class="d-flex align-items-center justify-content-center">
                            <i class="material-icons">flight_takeoff</i> Depature Date</label>
                        </div>
                        <input type="text" class="text-center" id="departure_date">
                    </div>
                    <div class="col-6">
                        <div class="d-flex align-items-center justify-content-center">
                            <i class="material-icons">flight_takeoff</i> Retrun Date</label>
                        </div>
                        <input type="text" class="text-center" id="return_date">
                    </div>
                </div>
            </div>

            <div class="card-footer">
                <button type="submit" class="btn btn-primary">Submit</button>
            </div>

        </form>
    </div>

</template>


