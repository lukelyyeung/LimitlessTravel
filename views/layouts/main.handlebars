{{!-- <script src="js/sr.js"></script> --}}
<script src="js/index.js"></script>
<script>
    window.sr = ScrollReveal();
    sr.reveal('.navbar', {
        duration: 1000,
        origin: 'bottom'
    });

    sr.reveal('#showcase h1', {
        duration: 1000,
        origin: 'right',
        distance: '300px',
    });

    sr.reveal('#showcase p', {
        duration: 1000,
        origin: 'left',
        distance: '300px',
    });

    sr.reveal('.showcase-btn', {
        duration: 1000,
        delay: 1000,
        origin: 'bottom'
    });

    sr.reveal('#testimonial div', {
        duration: 1000,
        origin: 'bottom'
    });

    sr.reveal('.info-left', {
        duration: 1000,
        origin: 'left',
        distance: '300px',
        viewFactor: 0.2
    });

    sr.reveal('.info-right', {
        duration: 1000,
        origin: 'right',
        distance: '300px',
        viewFactor: 0.2
    });

    sr.reveal('.form-group', {
        duration: 1000,
        origin: 'right',
        distance: '300px',

    });
</script>

</html>